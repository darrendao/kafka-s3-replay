[default]
debug=true
filebufferpath=/var/tmp/kafka-s3-go-consumer
maxchunksizebytes=1000000
maxchunkagemins=5
pollsleepmillis=10

[kafka]
host=127.0.0.1
port=9092
maxmessagesize=4096
topics=mytopic1,mytopic2
partitions=0,0

[s3]
bucket=kaas-kafka-backup
region=us-west-1
accesskey=ACCESS
secretkey=SECRET
